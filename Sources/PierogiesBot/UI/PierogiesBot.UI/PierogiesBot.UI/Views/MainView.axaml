<UserControl
    Background="#171C2C"
    Name="Root"
    d:DataContext="{x:Static views:DesignData.MainViewModel}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d"
    x:Class="PierogiesBot.UI.Views.MainView"
    x:DataType="mainView:PierogiesBotMainViewModel"
    xmlns="https://github.com/avaloniaui"
    xmlns:crontabRules="clr-namespace:PierogiesBot.UI.Views.CrontabRules"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mainView="clr-namespace:PierogiesBot.UI.ViewModels.Features.MainView;assembly=PierogiesBot.UI.ViewModels"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:reactiveUi="http://reactiveui.net"
    xmlns:views="clr-namespace:PierogiesBot.UI.Views"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <reactiveUi:RoutedViewHost
        HorizontalAlignment="Stretch"
        Router="{Binding Router}"
        VerticalAlignment="Stretch">
        <reactiveUi:RoutedViewHost.DefaultContent>
            <views:LoginView />
        </reactiveUi:RoutedViewHost.DefaultContent>
    </reactiveUi:RoutedViewHost>

</UserControl>